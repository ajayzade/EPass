<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.rawtalent.epass_user">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.rawtalent.epass_user.activity.RedirectActivity"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity
            android:name="com.rawtalent.epass_user.activity.UserPasses"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name="com.rawtalent.epass_user.activity.CreateEPass" />
        <activity
            android:name="com.rawtalent.epass_user.activity.ProfileActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name="com.rawtalent.epass_user.activity.StudentDetails" />
        <activity android:name="com.rawtalent.epass_user.activity.CreateNewAccount" />
        <activity android:name="com.rawtalent.epass_user.activity.LoginActivity" />
        <activity
            android:name="com.rawtalent.epass_user.activity.SplashscreenActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.rawtalent.epass_user.activity.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar" />
    </application>

</manifest>