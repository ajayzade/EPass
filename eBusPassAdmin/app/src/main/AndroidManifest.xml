<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.rawtalent.epass_admin">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.rawtalent.epass_admin.activity.ProfileActivity"></activity>
        <activity android:name="com.rawtalent.epass_admin.activity.VerifyEpass" />
        <activity android:name="com.rawtalent.epass_admin.activity.VerifyAccountDetails" />
        <activity
            android:name="com.rawtalent.epass_admin.activity.AccountVerification"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.rawtalent.epass_admin.activity.EpassApplications"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.rawtalent.epass_admin.activity.LoginActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.rawtalent.epass_admin.activity.SplashscreenActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.rawtalent.epass_admin.activity.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar" />
    </application>

</manifest>